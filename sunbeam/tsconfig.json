{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "es2020",
    "lib": ["es2021", "DOM"],
    "target": "es2021",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false
  },
  "exclude": ["node_modules", "dist"],
  "extends": "./.svelte-kit/tsconfig.json"
}
